{
  "name": "uilematisch",
  "version": "1.0.0",
  "private": true,
  "description": "A discord bot I develop for myself when I'm bored.",
  "keywords": [
    "nodejs",
    "typescript",
    "discord",
    "discordjs",
    "discord-bot",
    "discord-js-bot"
  ],
  "bugs": {
    "url": "https://github.com/CielSachen/Uilematisch/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CielSachen/Uilematisch.git"
  },
  "license": "ISC",
  "author": "Ciel",
  "type": "module",
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "pnpm register:dev && pnpm start:dev",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "register": "node ./dist/scripts/deploy.js",
    "register:dev": "NODE_ENV=development tsx ./src/scripts/deploy.ts",
    "start": "node ./dist/index.js",
    "start:dev": "NODE_ENV=development tsx watch ./src/index.ts"
  },
  "dependencies": {
    "@cielsachen/tenor-v2-wrapper": "^1.0.0",
    "@discordjs/opus": "^0.9.0",
    "@discordjs/voice": "^0.16.1",
    "@distube/soundcloud": "^1.3.3",
    "@distube/spotify": "^1.5.2",
    "@distube/yt-dlp": "^1.1.3",
    "canvas": "^2.11.2",
    "common-tags": "^1.8.2",
    "discord.js": "^14.14.1",
    "distube": "^4.1.1",
    "dotenv": "^16.4.5",
    "libsodium-wrappers": "^0.7.13",
    "luxon": "^3.4.4",
    "mongoose": "^8.2.1",
    "pokedex-promise-v2": "^4.2.0",
    "winston": "^3.12.0",
    "winston-console-format": "^1.0.8",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^1.6.3",
    "@types/common-tags": "^1.8.4",
    "@types/eslint": "^8.56.5",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.11.25",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsdoc": "^48.2.1",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "tsc-alias": "^1.8.8",
    "tslib": "^2.6.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.2"
  },
  "engines": {
    "node": ">=20.11.1",
    "pnpm": ">=8"
  }
